variable = { 'a' .. 'z' | 'A' .. 'Z' }
number = { ('0' .. '9')+ }
peren = { "(" | ")" }
WHITESPACE = _{ " " | "\n" | "\t" }
operation = _{ add | subtract | multiply | divide | power }
    add      = { "+" }
    subtract = { "-" }
    multiply = { "*" }
    divide   = { "/" }
    power    = { "^" }
equation = { 
    (number | variable) ~ operation ~ (number | equation | variable)
}